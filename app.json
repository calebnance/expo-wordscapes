{
  "expo": {
    "name": "Expo Wordscapes",
    "description": "wordscapes clone with expo",
    "scheme": "wordscapesclone",
    "slug": "wordscapesclone",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "version": "0.0.1",
    "platforms": ["android", "ios"],
    "githubUrl": "https://github.com/calebnance/expo-wordscapes",
    "orientation": "portrait",
    "primaryColor": "#3a71ae",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#3a71ae",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 1
    },
    "ios": {
      "buildNumber": "0.0.1",
      "supportsTablet": true
    }
  }
}
